def update_config name, value
    c=ConfigParams.find_by_name(name)
    if c.nil?
        ConfigParams.create(name:name,value:value)
    else
        c.update(value:value)
    end
end

def region name, parent
    c=Region.find_by_name(name)
    if c.nil?
     Region.create(name:name,parent_region:parent)
    end    
end


def category name, parent
    c=Category.find_by_name(name)
    if c.nil?
     Category.create(name:name,parent_category:parent)
    end    
end

def language longname, shortname
    l=Language.find_by_shortname(shortname)
    if l.nil?
        Language.create(shortname:shortname,longname:longname)
    end
end




update_config("vote_power",10)
update_config("best",1000)
update_config("trending",30)
update_config("amount_per_page",24)
update_config("reportev","0.12")
update_config("reportco","0.12")

